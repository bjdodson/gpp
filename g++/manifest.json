{
  "name": "g++",
  "version": "1.0",
  "description": "MobiSocial Google+ Embedding",
  "background_page": "background.html",
  "permissions": [
    "tabs"
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
